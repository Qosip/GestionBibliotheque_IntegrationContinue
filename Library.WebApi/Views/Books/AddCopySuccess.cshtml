@{
    ViewData["Title"] = "Exemplaires ajoutés";
}

<h2>@ViewData["Title"]</h2>

<div class="alert alert-success mt-3">
    Les exemplaires ont été ajoutés avec succès.
</div>

<a asp-action="AddCopy" class="btn btn-primary mt-3">Ajouter d'autres exemplaires</a>
